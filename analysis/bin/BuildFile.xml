<bin name="selectDeepJet" file="selectDeepJet.cc">
  <use name="root"/>
  <use name="rootxml"/>
  <lib name="Eve"/>
  <lib name="EG"/>
  <use name="delphys/analysis"/>
  <use name="delphys/external"/>
  <flags CXXFLAGS="-I/cms/ldap_home/yyoun/delPhys/src/delphys/external/delphes"/>
  <flags CXXFLAGS="-L/cms/ldap_home/yyoun/delPhys/src/delphys/external/delphes"/>
  <flags CXXFLAGS="-lDelphes"/>
</bin>

